<project name="imperial-sencillosolicitud-ws" default="ws-generate" basedir=".">
    <!-- Base URL used to download WebService WSDL and XSD files -->
    <property name="ws.url" value="http://localhost:8080/sencillosolicitud"/>

    <!-- Name of the WebService client [Plugin]WebService -->
    <property name="ws.name" value="SencilloSolicitudWebService"/>

    <!-- Directory where the WebService client is generated -->
    <property name="src.dir" value="../src"/>

    <!-- Java package of the generated WebService client -->
    <property name="ws.package" value="cl.imperial.sencillosolicitud.ws"/>

    <!-- Import common ant ws generation tasks -->
    <import file="../../Deploy/ant/build-ws.xml"/>
</project>